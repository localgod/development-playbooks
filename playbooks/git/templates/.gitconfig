[push]
  default = simple
[user]
  user = {{ git['user'] }}
  email = {{ git['email'] }}
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yello
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = red
[log]
  data = iso
[http]
  proxy = http://127.0.0.1:3128
[https]
  proxy = http://127.0.0.1:3128
[alias]
  b = branch
  c = commit
  co = checkout
  st = status
  cl = clone
